
<%= form_for @bucket,  html: { multipart: true } do |f| %>
	<%= f.label :image%>
	<%= f.file_field :image %>
	<%= f.label :title%>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :completed %>
  <%= f.select :completed, [true, false] %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.submit %>
<%end%>